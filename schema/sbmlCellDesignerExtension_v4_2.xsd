<xs:schema targetNamespace="http://www.sbml.org/2001/ns/celldesigner" xmlns:celldesigner="http://www.sbml.org/2001/ns/celldesigner" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <!--Simple data types.-->
  <xs:simpleType name="SId">
    <xs:annotation>
      <xs:documentation>The type SId is the same as the SId in SBML level 2 version 1.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(_|[a-z]|[A-Z])(_|[a-z]|[A-Z]|[0-9])*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SIdSet">
    <xs:annotation>
      <xs:documentation>A set of SId.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(_|[a-z]|[A-Z])(_|[a-z]|[A-Z]|[0-9])*(,(_|[a-z]|[A-Z])(_|[a-z]|[A-Z]|[0-9])*)*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="point2D">
    <xs:annotation>
      <xs:documentation>Coordinate value x,y.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(\+|-)?[0-9]+(\.[0-9]*)?((e|E)(\+|-)?[0-9]+)?,(\+|-)?[0-9]+(\.[0-9]*)?((e|E)(\+|-)?[0-9]+)?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="point2DList" final="list">
    <xs:annotation>
      <xs:documentation>List of coordinate values.</xs:documentation>
    </xs:annotation>
    <xs:list itemType="celldesigner:point2D"/>
  </xs:simpleType>
  <!--Types commonly used.-->
  <xs:complexType name="bounds">
    <xs:annotation>
      <xs:documentation>Bound of an object on canvas.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="h" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="w" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="x" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal"/>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="y" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="point">
    <xs:annotation>
      <xs:documentation>Point of an object on canvas.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="x" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal"/>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="y" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="innerPosition">
    <xs:annotation>
      <xs:documentation>Innerposition relative to a surrounding object.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="x" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal"/>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="y" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="boxSize">
    <xs:annotation>
      <xs:documentation>Box size. Width and height.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="height" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="width" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="singleLine">
    <xs:annotation>
      <xs:documentation>Single line type to draw object.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="width" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="doubleLine">
    <xs:annotation>
      <xs:documentation>Double line type to draw object.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="innerWidth" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="outerWidth" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="thickness" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:minInclusive value="0.0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="paint">
    <xs:annotation>
      <xs:documentation>Paint scheme to paint object.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="color" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="[0-9a-fA-F]{8}"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="scheme" use="optional">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="Color"/>
          <xs:enumeration value="Gradation"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="view">
    <xs:annotation>
      <xs:documentation>View state.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="state" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="brief"/>
          <xs:enumeration value="usual"/>
          <xs:enumeration value="complexnoborder"/>
          <xs:enumeration value="complexparentbrief"/>
          <xs:enumeration value="none"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="usualView">
    <xs:annotation>
      <xs:documentation>Data for drawing object with usual view.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="innerPosition" type="celldesigner:innerPosition" minOccurs="0"/>
      <xs:element name="boxSize" type="celldesigner:boxSize"/>
      <xs:element name="singleLine" type="celldesigner:singleLine"/>
      <xs:element name="paint" type="celldesigner:paint"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="briefView">
    <xs:annotation>
      <xs:documentation>Data for drawing object with brief (compact) view.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="innerPosition" type="celldesigner:innerPosition"/>
      <xs:element name="boxSize" type="celldesigner:boxSize"/>
      <xs:element name="singleLine" type="celldesigner:singleLine"/>
      <xs:element name="paint" type="celldesigner:paint"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="notes">
    <xs:annotation>
      <xs:documentation>Notes as in sbml.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any namespace="http://www.w3.org/1999/xhtml" processContents="skip" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="class">
    <xs:annotation>
      <xs:documentation>Object classes for network nodes or containers of process diagrams.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="PROTEIN"/>
        <xs:enumeration value="GENE"/>
        <xs:enumeration value="RNA"/>
        <xs:enumeration value="ANTISENSE_RNA"/>
        <xs:enumeration value="PHENOTYPE"/>
        <xs:enumeration value="ION"/>
        <xs:enumeration value="SIMPLE_MOLECULE"/>
        <xs:enumeration value="DRUG"/>
        <xs:enumeration value="UNKNOWN"/>
        <xs:enumeration value="COMPLEX"/>
        <xs:enumeration value="SQUARE"/>
        <xs:enumeration value="OVAL"/>
        <xs:enumeration value="SQUARE_CLOSEUP_NORTHWEST"/>
        <xs:enumeration value="SQUARE_CLOSEUP_NORTHEAST"/>
        <xs:enumeration value="SQUARE_CLOSEUP_SOUTHWEST"/>
        <xs:enumeration value="SQUARE_CLOSEUP_SOUTHEAST"/>
        <xs:enumeration value="SQUARE_CLOSEUP_NORTH"/>
        <xs:enumeration value="SQUARE_CLOSEUP_EAST"/>
        <xs:enumeration value="SQUARE_CLOSEUP_WEST"/>
        <xs:enumeration value="SQUARE_CLOSEUP_SOUTH"/>
        <xs:enumeration value="DEGRADED"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="name">
    <xs:annotation>
      <xs:documentation>Name of a compartment, or a species.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="activity">
    <xs:annotation>
      <xs:documentation>Designation of activity of the species (aliases).</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="active"/>
        <xs:enumeration value="inactive"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <!--Species extension.-->
  <xs:element name="positionToCompartment">
    <xs:annotation>
      <xs:documentation>Species' position to its parent compartment. For the species not forming any complex.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="outerSurface"/>
        <xs:enumeration value="transmembrane"/>
        <xs:enumeration value="innerSurface"/>
        <xs:enumeration value="inside"/>
        <xs:enumeration value="insideOfMembrane"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="complexSpecies" type="celldesigner:SId">
    <xs:annotation>
      <xs:documentation>Species id of the parent complex. For the species forming a complex.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="hypothetical">
    <xs:annotation>
      <xs:documentation>Designator to tell that the species is hypothetical.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:boolean"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="proteinReference">
    <xs:annotation>
      <xs:documentation>Reference to a protein.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="celldesigner:SId"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="geneReference">
    <xs:annotation>
      <xs:documentation>Reference to a gene.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="celldesigner:SId"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="rnaReference">
    <xs:annotation>
      <xs:documentation>Reference to an rna.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="celldesigner:SId"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="antisensernaReference">
    <xs:annotation>
      <xs:documentation>Reference to an antisense rna.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="celldesigner:SId"/>
    </xs:simpleType>
  </xs:element>
  <xs:element name="homodimer">
    <xs:annotation>
      <xs:documentation>Valence value of homogeneous multimer.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:positiveInteger"/>
    </xs:simpleType>
  </xs:element>
  <xs:complexType name="listOfModifications">
    <xs:annotation>
      <xs:documentation>List of residue modification.</xs:documentation>
    </xs:annotation>
    <xs:sequence maxOccurs="unbounded">
      <xs:element name="modification">
        <xs:complexType>
          <xs:attribute name="residue" type="celldesigner:SId" use="required"/>
          <xs:attribute name="state" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="phosphorylated"/>
                <xs:enumeration value="acetylated"/>
                <xs:enumeration value="ubiquitinated"/>
                <xs:enumeration value="methylated"/>
                <xs:enumeration value="hydroxylated"/>
                <xs:enumeration value="don't care"/>
                <xs:enumeration value="unknown"/>
                <xs:enumeration value="glycosylated"/>
                <xs:enumeration value="myristoylated"/>
                <xs:enumeration value="palmytoylated"/>
                <xs:enumeration value="prenylated"/>
                <xs:enumeration value="protonated"/>
                <xs:enumeration value="sulfated"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="state">
    <xs:annotation>
      <xs:documentation>Chemical "state" to distinguish chemical species derived from a root chemical.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="celldesigner:homodimer" minOccurs="0"/>
      <xs:element name="listOfModifications" type="celldesigner:listOfModifications" minOccurs="0"/>
      <xs:element name="listOfStructuralStates" type="celldesigner:listOfStructuralStates" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="speciesIdentity">
    <xs:annotation>
      <xs:documentation>Identity to distinguish chemical species of CellDesigner's model.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="celldesigner:class"/>
      <xs:element ref="celldesigner:hypothetical" minOccurs="0"/>
      <xs:choice>
        <xs:element ref="celldesigner:name" minOccurs="0"/>
        <xs:element ref="celldesigner:proteinReference" minOccurs="0"/>
        <xs:element ref="celldesigner:rnaReference" minOccurs="0"/>
        <xs:element ref="celldesigner:geneReference" minOccurs="0"/>
        <xs:element ref="celldesigner:antisensernaReference" minOccurs="0"/>
      </xs:choice>
      <xs:element name="state" type="celldesigner:state" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="catalyzed">
    <xs:annotation>
      <xs:documentation>Catalyzed reaction.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="reaction" type="celldesigner:SId" use="required"/>
  </xs:complexType>
  <xs:complexType name="listOfCatalyzedReactions">
    <xs:annotation>
      <xs:documentation>List of catalyzed reaction.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="catalyzed" type="celldesigner:catalyzed" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="speciesAnnotationType">
    <xs:annotation>
      <xs:documentation>Annotation for species.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="extension">
        <xs:complexType>
          <xs:sequence>
            <xs:choice>
              <xs:element ref="celldesigner:positionToCompartment"/>
              <xs:element ref="celldesigner:complexSpecies"/>
            </xs:choice>
            <xs:element name="speciesIdentity" type="celldesigner:speciesIdentity"/>
            <xs:element name="listOfCatalyzedReactions" type="celldesigner:listOfCatalyzedReactions" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- keep the possibility to add RDF annotations -->
      <xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <!--Model extension.-->
  <xs:element name="modelVersion" fixed="4.0">
    <xs:annotation>
      <xs:documentation>Model version declaration.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:decimal">
        <xs:minInclusive value="0.0"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:complexType name="modelDisplay">
    <xs:annotation>
      <xs:documentation>Model size, width and height.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="sizeX" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:short">
          <xs:minExclusive value="0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="sizeY" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:short">
          <xs:minExclusive value="0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <!--Model.IncludedSpecies-->
  <xs:complexType name="species">
    <xs:annotation>
      <xs:documentation>Species inside complexes.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="notes" type="celldesigner:notes" minOccurs="0"/>
      <xs:element name="annotation">
        <xs:complexType>
          <xs:sequence>
            <xs:choice>
              <xs:element ref="celldesigner:positionToCompartment"/>
              <xs:element ref="celldesigner:complexSpecies"/>
            </xs:choice>
            <xs:element name="speciesIdentity" type="celldesigner:speciesIdentity"/>
            <xs:element name="listOfCatalyzedReactions" type="celldesigner:listOfCatalyzedReactions" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="id" type="celldesigner:SId" use="required"/>
    <xs:attribute name="name" type="xs:string" use="optional"/>
    <xs:attribute name="compartment" type="celldesigner:SId"/>
    <xs:attribute name="initialAmount" type="xs:double" use="optional"/>
    <xs:attribute name="initialConcentration" type="xs:double" use="optional"/>
    <xs:attribute name="substanceUnits" type="celldesigner:SId" use="optional"/>
    <xs:attribute name="spatialSizeUnits" type="celldesigner:SId" use="optional"/>
    <xs:attribute name="hasOnlySubstanceUnits" type="xs:boolean" use="optional" default="false"/>
    <xs:attribute name="boundaryCondition" type="xs:boolean" use="optional" default="false"/>
    <xs:attribute name="charge" type="xs:integer" use="optional"/>
    <xs:attribute name="constant" type="xs:boolean" use="optional" default="false"/>
  </xs:complexType>
  <xs:complexType name="listOfIncludedSpecies">
    <xs:annotation>
      <xs:documentation>List of species included by complexes.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="species" type="celldesigner:species" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <!--Model.CompartmentAlias-->
  <xs:complexType name="compartmentAlias">
    <xs:annotation>
      <xs:documentation>For compartment aliases.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="celldesigner:class"/>
      <xs:choice>
        <xs:element name="bounds" type="celldesigner:bounds" minOccurs="0"/>
        <xs:element name="point" type="celldesigner:point" minOccurs="0"/>
      </xs:choice>
      <xs:element name="namePoint" type="celldesigner:point"/>
      <xs:element name="doubleLine" type="celldesigner:doubleLine"/>
      <xs:element name="paint" type="celldesigner:paint"/>
      <xs:element name="info" type="celldesigner:info"/>
    </xs:sequence>
    <xs:attribute name="compartment" use="required">
      <xs:simpleType>
        <xs:restriction base="celldesigner:SId"/>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="id" use="required">
      <xs:simpleType>
        <xs:restriction base="celldesigner:SId"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="listOfCompartmentAliases">
    <xs:annotation>
      <xs:documentation>List of compartment aliases.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="compartmentAlias" type="celldesigner:compartmentAlias" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <!--Model.ComplexSpeciesAlias, Model.SpeciesAlias-->
  <xs:complexType name="complexSpeciesAlias">
    <xs:annotation>
      <xs:documentation>For species aliases of complex species.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="celldesigner:activity"/>
      <xs:element name="bounds" type="celldesigner:bounds"/>
      <xs:element name="font">
        <xs:complexType>
          <xs:attribute name="size" type="xs:int"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="view" type="celldesigner:view"/>
      <xs:element name="backupSize">
        <xs:complexType>
          <xs:attribute name="w" type="xs:double"/>
          <xs:attribute name="h" type="xs:double"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="backupView" type="celldesigner:view"/>
      <xs:element name="structuralState" type="celldesigner:structuralStateAngle" minOccurs="0"/>
      <xs:element name="usualView" type="celldesigner:usualView"/>
      <xs:element name="briefView" type="celldesigner:briefView"/>
      <xs:element name="listOfSpeciesTag" type="celldesigner:listOfSpeciesTag" minOccurs="0"/>
      <xs:element name="info" type="celldesigner:info"/>
    </xs:sequence>
    <xs:attribute name="compartmentAlias" use="optional">
      <xs:simpleType>
        <xs:restriction base="celldesigner:SId"/>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="id" use="required">
      <xs:simpleType>
        <xs:restriction base="celldesigner:SId"/>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="species" type="celldesigner:SId" use="required"/>
  </xs:complexType>
  <xs:complexType name="listOfComplexSpeciesAliases">
    <xs:annotation>
      <xs:documentation>List of complex species aliases.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="complexSpeciesAlias" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="celldesigner:complexSpeciesAlias">
              <xs:attribute name="complexSpeciesAlias" type="celldesigner:SId" use="optional"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="speciesAlias">
    <xs:annotation>
      <xs:documentation>For species aliases not forming complexes.</xs:documentation>
